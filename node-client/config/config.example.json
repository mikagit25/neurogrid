{
  "coordinator_url": "http://localhost:3001",
  "node_token": "your-node-token-here",
  "node_id": "auto-generate",
  "max_vram_gb": 8,
  "max_cpu_cores": 4,
  "supported_models": ["text", "image"],
  "data_dir": "./data",
  "log_level": "INFO",
  "heartbeat_interval": 30,
  "task_timeout": 300,
  "enable_docker": true,
  "docker_network": "neurogrid",
  "docker_base_image": "neurogrid/ai-worker:latest",
  "enable_gpu_passthrough": true,
  "max_concurrent_containers": 3,
  "enable_container_security": true,
  "docker_user_namespace": "neurogrid",
  "metrics_port": 8080,
  "metrics_interval": 60,
  "model_cache_size_gb": 20,
  "model_cache_dir": "data/models",
  "auto_download_models": true,
  "preferred_models": [],
  "encrypt_data": true,
  "enable_encryption": true,
  "crypto_dir": "data/crypto",
  "key_rotation_hours": 24,
  "jwt_secret": "auto-generate",
  "jwt_expiry_hours": 1,
  "allow_custom_models": false,
  "sandbox_mode": true,
  "detailed_metrics": true,
  "metrics_history_hours": 24,
  "concurrent_downloads": 2,
  "max_retries": 5,
  "retry_backoff": 2.0,
  "rate_limit_per_minute": 60
}