# Production environment variables
# These URLs will be set dynamically based on hostname in api.js
# Leave empty to use automatic detection
NEXT_PUBLIC_API_URL=
NEXT_PUBLIC_WS_URL=
NEXT_PUBLIC_APP_URL=

NODE_ENV=production
NEXT_PUBLIC_ENV=production

# Production features
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_MONITORING=true
NEXT_PUBLIC_ENABLE_ADMIN=false
NEXT_PUBLIC_ENABLE_DEBUG=false

# Site info
NEXT_PUBLIC_SITE_NAME="NeuroGrid - Decentralized AI Computing Platform"
NEXT_PUBLIC_SITE_DESCRIPTION="Democratizing AI computing through decentralized GPU networks"
NEXT_PUBLIC_SITE_URL=https://neurogrid.network
