{
  "name": "neurogrid-coordinator",
  "version": "0.1.0",
  "description": "NeuroGrid Coordinator Server - Central management for distributed AI inference",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "start:production": "NODE_ENV=production node src/app-production.js",
    "dev": "nodemon src/app.js",
    "dev:simple": "node src/app-simple.js",
    "test": "jest --config=jest.config.json",
    "test:watch": "jest --config=jest.config.json --watch",
    "test:coverage": "jest --config=jest.config.json --coverage",
    "test:unit": "jest --config=jest.config.json --testPathPattern=tests/unit",
    "test:integration": "jest --config=jest.config.json --testPathPattern=tests/integration",
    "test:e2e": "jest --config=jest.config.json --testPathPattern=tests/e2e",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:ci": "jest --config=jest.config.json --coverage --ci --watchAll=false --passWithNoTests",
    "build": "echo 'No build step required for Node.js'",
    "lint": "eslint src/ tests/ --ext .js",
    "lint:fix": "eslint src/ tests/ --ext .js --fix",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "pretest": "npm run lint",
    "posttest": "npm run test:coverage"
  },
  "keywords": [
    "ai",
    "inference",
    "distributed",
    "coordinator",
    "blockchain",
    "gpu"
  ],
  "author": "NeuroGrid Team",
  "license": "MIT",
  "dependencies": {
    "@tensorflow/tfjs-node": "^4.22.0",
    "archiver": "^6.0.1",
    "axios": "^1.12.2",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.3",
    "compression": "^1.7.4",
    "connect-redis": "^7.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.22.1",
    "express-prometheus-middleware": "^1.2.0",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.18.2",
    "express-slow-down": "^3.0.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.2.0",
    "ioredis": "^5.3.2",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "node-cron": "^3.0.3",
    "node-os-utils": "^2.0.1",
    "nodemailer": "^7.0.10",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.4",
    "redis": "^5.9.0",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3",
    "speakeasy": "^2.0.0",
    "stripe": "^14.25.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "twilio": "^5.10.3",
    "uuid": "^9.0.1",
    "validator": "^13.15.15",
    "winston": "^3.11.0",
    "ws": "^8.18.3",
    "xss": "^1.0.15",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.2",
    "concurrently": "^9.2.1",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^30.2.0",
    "jest-extended": "^6.0.0",
    "nodemon": "^3.0.2",
    "nyc": "^17.1.0",
    "prettier": "^3.1.0",
    "sqlite3": "^5.1.7",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
